# GitLab instance API endpoint
GITLAB_API_URL=https://gitlab.company.local/api/v4

# Private access token with group creation rights
GITLAB_TOKEN=token

# Set to 'true' to allow curl to skip SSL certificate validation (useful for self-signed certs during testing).
# Leave empty or comment out to enforce SSL validation (recommended for production).
CURL_INSECURE=true

# Parent group ID if nested under another group, use "null" for top-level groups
PARENT_GROUP_ID=null

# Comma-separated list of group names to create
GROUP_NAMES=Backend-developer,DevOps